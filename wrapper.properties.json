    "irc-enabled": false,
    "nick": "MinecraftWrap",
    "obstruct-nicknames": false,
    "password": "from console use `/password IRC password <your password>`",
    "password-plaintext": false,
    "port": 6667,
    "server": "benbaptist.com",
    "show-channel-server": true,
    "show-irc-join-part": true
